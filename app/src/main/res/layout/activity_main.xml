<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="173dp"
        android:layout_marginTop="58dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="654dp"
        android:text="Rutgers Cafe"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/DonutButton"
        android:layout_width="196dp"
        android:layout_height="166dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="199dp"
        android:layout_marginBottom="399dp"
        android:adjustViewBounds="true"
        android:contentDescription="Donuts"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/donuts" />

    <ImageButton
        android:id="@+id/CoffeeButton"
        android:layout_width="166dp"
        android:layout_height="163dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="401dp"
        android:adjustViewBounds="true"
        android:contentDescription="Coffee"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/coffee" />

    <ImageButton
        android:id="@+id/StoreOrderButton"
        android:layout_width="192dp"
        android:layout_height="179dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="203dp"
        android:layout_marginBottom="205dp"
        android:adjustViewBounds="true"
        android:contentDescription="StoreOrders"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/store" />

    <ImageButton
        android:id="@+id/OrderButton"
        android:layout_width="163dp"
        android:layout_height="175dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="206dp"
        android:adjustViewBounds="true"
        android:contentDescription="StoreOrders"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/StoreOrderButton"
        app:srcCompat="@drawable/basket" />
</androidx.constraintlayout.widget.ConstraintLayout>